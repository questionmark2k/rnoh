{
  "name": "react-native-harmony-sample-package",
  "version": "1.0.0",
  "description": "Sample RN harmony package",
  "main": "index.ts",
  "scripts": {
    "install:rn_harmony": "cd ../react-native-harmony && npm pack && cd ../react-native-harmony-sample-package && npm uninstall react-native-harmony && npm i react-native-harmony@file:../react-native-harmony/rnoh-react-native-harmony-0.72.12.tgz",
    "prepack": "npm i && npm run pack:harmony",
    "pack:harmony": "react-native pack-harmony --oh-module-path ../tester/harmony/sample_package --harmony-dir-path ./harmony --package-json-path ./package.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "harmony": {
    "alias": "sample-package"
  },
  "files": [
    "index.ts",
    "src/*",
    "harmony.tar.gz"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-harmony": "*"
  },
  "devDependencies": {
    "@types/react": "^18.2.47",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-harmony": "file:../react-native-harmony/rnoh-react-native-harmony-0.72.12.tgz"
  }
}
